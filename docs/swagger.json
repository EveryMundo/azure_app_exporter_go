{
    "swagger": "2.0",
    "info": {
        "description": "Expose Prometheus metrics for expiring Azure password credentials",
        "title": "Azure app exporter",
        "contact": {},
        "version": "0.1.0"
    },
    "paths": {
        "/api/apps": {
            "get": {
                "description": "Show all Azure applications cached in the exporter (truncated in Swagger UI to 50 entries)\n\nCall this endpoint outside Swagger UI to see full response",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "applications"
                ],
                "summary": "Show all Azure applications cached in the exporter (truncated in Swagger UI to 50 entries)",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "$ref": "#/definitions/datatypes.AzureApplication"
                            }
                        }
                    }
                }
            }
        },
        "/api/apps/{id}": {
            "get": {
                "description": "Show Azure application by ID",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "applications"
                ],
                "summary": "Show Azure application by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "ID of Azure application to lookup",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/datatypes.AzureApplication"
                        }
                    }
                }
            }
        },
        "/api/settings": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "info"
                ],
                "summary": "Show the exporter settings, except sensitive values",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/appsettings.Settings"
                        }
                    }
                }
            }
        },
        "/licenses": {
            "get": {
                "description": "Show licenses\n\nGenerated by go-licenses",
                "produces": [
                    "text/plain",
                    "application/csv"
                ],
                "tags": [
                    "info"
                ],
                "summary": "Show licenses",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/metrics": {
            "get": {
                "description": "Show the Prometheus metrics (truncated in Swagger UI to 20KiB)\n\nCall this endpoint outside Swagger UI to see full response",
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "Show the Prometheus metrics (truncated in Swagger UI to 20KiB)",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "appsettings.Applications": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "x-order": "1"
                },
                "cache_refresh_interval": {
                    "type": "string",
                    "x-order": "2",
                    "example": "15m"
                },
                "url": {
                    "type": "string",
                    "x-order": "2"
                },
                "results_per_page": {
                    "type": "integer",
                    "maximum": 999,
                    "minimum": 1,
                    "x-order": "3"
                }
            }
        },
        "appsettings.Credentials": {
            "type": "object",
            "properties": {
                "tenant_id": {
                    "type": "string",
                    "x-order": "1"
                },
                "client_id": {
                    "type": "string",
                    "x-order": "2"
                },
                "client_secret": {
                    "type": "string",
                    "x-order": "3"
                }
            }
        },
        "appsettings.Debug": {
            "type": "object",
            "properties": {
                "no_verify_tls": {
                    "type": "boolean"
                }
            }
        },
        "appsettings.Metrics": {
            "type": "object",
            "properties": {
                "prune_interval": {
                    "type": "string",
                    "x-nullable": true,
                    "x-order": "1",
                    "example": "30m"
                },
                "expand_unsupported_url_metrics": {
                    "type": "boolean",
                    "x-order": "2"
                }
            }
        },
        "appsettings.OpenApi": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "x-order": "1"
                },
                "docs_url": {
                    "type": "string",
                    "x-order": "2"
                },
                "swagger_ui_url": {
                    "type": "string",
                    "x-order": "3"
                }
            }
        },
        "appsettings.Settings": {
            "type": "object",
            "required": [
                "credentials"
            ],
            "properties": {
                "credentials": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Credentials"
                        }
                    ],
                    "x-order": "1"
                },
                "metrics": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Metrics"
                        }
                    ],
                    "x-order": "2"
                },
                "applications": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Applications"
                        }
                    ],
                    "x-order": "3"
                },
                "web": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Web"
                        }
                    ],
                    "x-order": "4"
                },
                "openapi": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.OpenApi"
                        }
                    ],
                    "x-order": "5"
                },
                "tls": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Tls"
                        }
                    ],
                    "x-order": "6"
                },
                "debug": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/appsettings.Debug"
                        }
                    ],
                    "x-order": "7"
                }
            }
        },
        "appsettings.Tls": {
            "type": "object",
            "properties": {
                "cipher_suites": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "x-order": "1",
                    "example": [
                        "TLS_ECDHE_RSA_WITH_CHACHA20_POLY1305_SHA256"
                    ]
                },
                "protocol_versions": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "x-order": "2",
                    "example": [
                        "TLS13"
                    ]
                }
            }
        },
        "appsettings.Web": {
            "type": "object",
            "properties": {
                "listen_address": {
                    "type": "string",
                    "x-order": "1"
                },
                "cert_file": {
                    "type": "string",
                    "x-nullable": true,
                    "x-order": "2"
                },
                "key_file": {
                    "type": "string",
                    "x-nullable": true,
                    "x-order": "3"
                }
            }
        },
        "datatypes.AzureApplication": {
            "type": "object",
            "required": [
                "appId",
                "id",
                "passwordCredentials"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "x-order": "1"
                },
                "appId": {
                    "type": "string",
                    "x-order": "2"
                },
                "displayName": {
                    "type": "string",
                    "x-nullable": true,
                    "x-order": "3"
                },
                "passwordCredentials": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/datatypes.PasswordCredential"
                    },
                    "x-order": "4"
                }
            }
        },
        "datatypes.PasswordCredential": {
            "type": "object",
            "required": [
                "keyId"
            ],
            "properties": {
                "keyId": {
                    "type": "string",
                    "x-order": "1"
                },
                "displayName": {
                    "type": "string",
                    "x-nullable": true,
                    "x-order": "2"
                },
                "endDateTime": {
                    "type": "string",
                    "format": "date-time",
                    "x-nullable": true,
                    "x-order": "3"
                }
            }
        }
    }
}